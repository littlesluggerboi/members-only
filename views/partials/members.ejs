<link rel="stylesheet" href="/css/members.css">
<div class="members">
    <div class="member header">
        <h3>Username</h3>
        <h3>Role</h3>
    </div>
    <%members.map((member)=>{%>
        <div class="member">
            <p><%=member.username%></p>
            <p><%=member.role%></p>
        </div>
    <%})%>
</div>